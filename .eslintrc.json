{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-var": "error",
    "no-dupe-keys": "error",
    "space-infix-ops": "error",
    "arrow-parens": [
      "error",
      "always"
    ]
  }
}
